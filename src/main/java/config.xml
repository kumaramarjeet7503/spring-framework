<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd
">

<bean class="com.core.Address" name="address1" init-method="init" >
	<constructor-arg index="0" value="4321432"  ></constructor-arg>
	<constructor-arg  index="1" value="Delhi"  ></constructor-arg>
	<constructor-arg index="2"  value="Mohan Garden" ></constructor-arg>
	<constructor-arg index="3" >
		<list >
			<value>Amarjeet</value>
			<value>Kumar</value>
			<value>Karishma</value>
		</list>
	</constructor-arg>  
</bean>	
	
	<bean class="com.core.Student" name="student1"   >
		<property name="Address" value="fdlksfsda" />
		<property name="Section" value="A" />
		<property name="StudentId" value="10" />
		<property name="StudentAddress">
			<ref bean="address1" ></ref>
		</property>
	</bean>


</beans>